<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.altamar.miguelangel.concesionario.Informe_3">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/lblInforme3"
        android:text="@string/lblInforme3"
        android:textColor="#009688" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/lblInforme3_1"
        android:layout_below="@+id/lblInforme3"
        android:text="@string/lblInforme3_1"
        android:textColor="#009688" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/txtCarrosAmarillos"
        android:layout_below="@+id/lblInforme3_1"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/lblInforme3_2"
        android:layout_below="@+id/txtCarrosAmarillos"
        android:text="@string/lblInforme3_2"
        android:textColor="#009688" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/txtCarrosRojos"
        android:layout_below="@+id/lblInforme3_2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/lblInforme3_3"
        android:layout_below="@+id/txtCarrosRojos"
        android:text="@string/lblInforme3_3"
        android:textColor="#009688" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/txtCarrosAzules"
        android:layout_below="@+id/lblInforme3_3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

</RelativeLayout>
